    </offsets>
  </model>

<!-- LANDING LIGHT CONE -->

  <model>
    <name>LandingLight</name>
    <path>Xlight-cone.xml</path>
    <condition>
      <equals>
        <property>gear/gear[0]/position-norm</property>
        <value>1</value>
      </equals>
    </condition>
    <offsets>
      <x-m> -3.12050 </x-m>
      <y-m> -0.13735 </y-m>
      <z-m> -2.56900 </z-m>
      <pitch-deg> -10.00000 </pitch-deg>
    </offsets>
  </model>

  <animation>
    <type>rotate</type>
    <object-name>LandingLight</object-name>
    <property>gear/gear[0]/caster-angle-deg</property>
    <factor> -0.8 </factor>
    <center>
      <x-m> -3.06209 </x-m>
      <y-m>  0.00000 </y-m>
      <z-m> -1.74141 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 0 </y>
      <z> 1 </z>
    </axis>
  </animation>

</PropertyList>
