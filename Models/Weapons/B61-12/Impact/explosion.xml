      <animation n="100">
        <type>scale</type>
        <property>constants/zero</property>
        <x-offset> 250 </x-offset>
        <y-offset> 250 </y-offset>
        <z-offset> 250 </z-offset>
      </animation>
    </overlay>
  </model>

  <model>
    <object-name>Therrmal</object-name>
    <path>ExplosionThermal.xml</path>
    <overlay>
      <animation n="100">
        <type>scale</type>
        <property>constants/zero</property>
        <x-offset> 250 </x-offset>
        <y-offset> 250 </y-offset>
        <z-offset> 250 </z-offset>
      </animation>
    </overlay>
  </model>

</PropertyList>
